{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "azure-test-factory"
		},
		"ABS_test_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ABS_test'"
		},
		"AST_test_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AST_test'"
		},
		"trigger2_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/e2b7c2e1-e8c9-42bc-ae75-13f0da78a759/resourceGroups/azure-test/providers/Microsoft.Storage/storageAccounts/azuredevcmnstorage"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "azure-test-factory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ABS_test_connectionString": {
						"value": "[parameters('ABS_test_connectionString')]"
					},
					"AST_test_connectionString": {
						"value": "[parameters('AST_test_connectionString')]"
					},
					"trigger2_properties_typeProperties_scope": {
						"value": "[parameters('trigger2_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}